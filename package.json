{
  "name": "eda-backend-deps",
  "version": "0.3.5",
  "description": "A dependency repo of backend libraries used in EDA's exercies and apps",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "watch:tests": "jest --watch",
    "seed": "sequelize-cli db:seed:all",
    "migrate": "sequelize-cli db:migrate",
    "rollback": "sequelize-cli db:migrate:undo",
    "server": "nodemon ../../server --watch ../../server"
  },
  "bin": {
    "test-backend": "./test",
    "watch-backend-tests": "./watch-tests",
    "seed-backend-db": "./seed",
    "migrate-backend-db": "./migrate",
    "rollback-backend-db": "./rollback",
    "start-backend-dev-server": "./server"
  },
  "main": "index.js",
  "homepage": "https://github.com/enspiral-dev-academy/eda-backend-deps#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enspiral-dev-academy/eda-backend-deps.git"
  },
  "bugs": {
    "url": "https://github.com/enspiral-dev-academy/eda-backend-deps/issues"
  },
  "keywords": [
    "backend"
  ],
  "jest": {
    "verbose": true,
    "rootDir": "../../server"
  },
  "dependencies": {
    "authenticare": "^0.4.7",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "regenerator-runtime": "^0.13.5",
    "sequelize": "^6.3.0",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2"
  }
}
