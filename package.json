{
  "name": "eda-backend-deps",
  "version": "0.2.7",
  "description": "A dependency repo of backend libraries used in EDA's exercies and apps",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "watch-tests": "jest --watch",
    "knex": "knex --knexfile ../../server/db/knexfile.js",
    "seed": "npm run knex seed:run",
    "migrate": "npm run knex migrate:latest",
    "rollback": "npm run knex migrate:rollback",
    "server": "nodemon ../../server --watch ../../server",
    "lint": "eslint ../../server"
  },
  "bin": {
    "lint-backend": "./lint",
    "test-backend": "./test",
    "watch-backend-tests": "./watch-tests",
    "seed-backend-db": "./seed",
    "migrate-backend-db": "./migrate",
    "rollback-backend-db": "./rollback",
    "start-backend-dev-server": "./server"
  },
  "homepage": "https://github.com/enspiral-dev-academy/eda-backend-deps#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enspiral-dev-academy/eda-backend-deps.git"
  },
  "bugs": {
    "url": "https://github.com/enspiral-dev-academy/eda-backend-deps/issues"
  },
  "keywords": [
    "backend"
  ],
  "jest": {
    "verbose": true,
    "rootDir": "../../server"
  },
  "dependencies": {
    "authenticare": "^0.4.7",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jest": "^26.1.0",
    "knex": "^0.21.1",
    "nodemon": "^2.0.4",
    "regenerator-runtime": "^0.13.5",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2"
  }
}
