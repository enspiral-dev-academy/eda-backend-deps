{
  "parser": "babel-eslint",
  "env": {
    "es2020": true,
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "import",
    "jest",
    "node",
    "prettier",
    "promise",
    "security"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jest/recommended",
    "plugin:jest/style",
    "plugin:promise/recommended",
    "plugin:security/recommended",
    "plugin:prettier/recommended"
  ]
}
